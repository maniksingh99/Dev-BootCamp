{
    "name": "Social Media Blocker",
    "version": "1.0",
    "description": "Block a site after 10 secs",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "browser_action": {
        "default_popup": "popup.html"
      },
      "permissions": [
        "tabs"
    ]
  }